{
    "name": "miw/demo-symfony-messenger",
    "description": "Demo componente symfony/messenger con broker RabbitMQ",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "https://miw.etsisi.upm.es/",
    "keywords": ["Symfony7", "amqp", "RabbitMQ", "symfony-messenger", "OpenAPI"],
    "authors": [
        {
            "name": "MiW UPM",
            "homepage": "https://www.etsisi.upm.es/",
            "email": "miw.etsisi@upm.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "symfony/amqp-messenger": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "*",
        "symfony/messenger": "*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "^7.1",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*",
            "docker": false
        }
    }
}

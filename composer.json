{
    "name": "miw/demo-symfony-messenger",
    "description": "Demo componente symfony/messenger con broker RabbitMQ",
    "type": "project",
    "license": "MIT",
    "homepage": "http://miw.etsisi.upm.es/",
    "keywords": ["Symfony6", "amqp", "RabbitMQ", "symfony-messenger", "OpenAPI"],
    "authors": [
        {
            "name": "AOS UPM",
            "homepage": "http://www.etsisi.upm.es/",
            "email": "miw.etsisi@upm.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/amqp-messenger": "^6.1",
        "symfony/console": "^6.1",
        "symfony/dotenv": "^6.1",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^6.1",
        "symfony/messenger": "^6.1",
        "symfony/monolog-bundle": "^3.8",
        "symfony/runtime": "^6.1",
        "symfony/yaml": "^6.1"
    },
    "require-dev": {
        "symfony/stopwatch": "^6.1",
        "symfony/web-profiler-bundle": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    }
}
